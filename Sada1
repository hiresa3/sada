if (isLoggedIn() && (duplicatePerks.length > 0 || hasNetflixSubscription)) {
    perks[2]((perk) => {
        if (isDuplicateSharablePerks(perk)) {
            duplicateDisclosureHandler(duplicatePerks);
        } else if (isSubscribedAsBundle(perk)) { // Changed from `else` to `else if`
            duplicateDisclosureHandler(netflixPerks);
        }
    });
}
