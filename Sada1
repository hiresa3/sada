    message = convertStringToElements(duplicatePerkOverlayAAL[perkDuplicateOverlay?.currentSpold]?.message);
} else {
    const overlayMessage = duplicatePerkOverlay[perkDuplicateOverlay?.currentSpold]?.message;
    if (overlayMessage) {
        message = convertStringToElements(overlayMessage);
    } else {
        // Assign a default value or handle the case when the message is not available
        message = /* default value or fallback */;
    }
}
